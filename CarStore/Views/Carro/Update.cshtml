@model Carro

<h2>Preencha os campos com os dados do veívulo </h2>
<form class="col-md-4" asp-action="Create">
    <div class="form-group">
        <label asp-for="marca" class="control-label"></label>
        <input asp-for="marca" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="modelo" class="control-label"></label>
        <input asp-for="modelo" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="preco" class="control-label"></label>
        <input asp-for="preco" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ano" class="control-label"></label>
        <input asp-for="ano" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="url1" class="control-label"></label>
        <input asp-for="url1" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="descricao" class="control-label"></label>
        <input asp-for="descricao" class="form-control" />
    </div>
    <div class="col-12">
        <input type="submit" class="btn btn-primary" value="Salvar mudanças" />
        <a class="btn btn-dark" asp-area="" asp-controller="Carro" asp-action="Index" role="button">Cancelar</a>
    </div>
</form>